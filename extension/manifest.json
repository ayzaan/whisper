{
  "name": "Whisper",
  "version": "1.0",
  "manifest_version": 1,
  "description": "whisper",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "minimum_chrome_version": "6",
  "permissions": [ "contextMenus", "tabs" ],
  "background": {
    "scripts": ["whisper_listener.js"],
    "persistent": false
  },
}