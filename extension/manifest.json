{
  "name": "Whisper",
  "version": "1.0",
  "manifest_version": 2,
  "description": "whisper",
  "browser_action": {
    "default_icon": "icon-bitty.png",
    "default_popup": "popup.html"
  },
  "minimum_chrome_version": "6",
  "permissions": [ "contextMenus", "tabs" ],
  "background": {
    "scripts": ["contextmenu.js"]
  },
  "icons": {
    "16": "icon-bitty.png",
    "32": "icon-small.png",
    "64": "icon-medium.png"
  }
}